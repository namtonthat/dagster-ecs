# Example Terraform variables file
# Copy this to terraform.tfvars and customize with your values
# IMPORTANT: Never commit terraform.tfvars to git - it contains sensitive data

# Basic configuration
aws_region    = "ap-southeast-2"
environment   = "dev"
vpc_cidr      = "10.0.0.0/16"
availability_zones = ["ap-southeast-2a", "ap-southeast-2b"]

# Database configuration
db_username = "dagster"
db_password = "your-secure-database-password-here"

# S3 bucket name (must be globally unique)
s3_bucket_name = "your-unique-dagster-bucket-name"

# Basic authentication for Dagster web interface
dagster_auth_user = "admin"
dagster_auth_password = "YourSecureDagsterPassword2024!"